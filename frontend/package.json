{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "webpack --mode development",
    "prod": "webpack --mode production",
    "live": "webpack serve --open --mode development --hot",
    "dev-test": "cross-env IS_TEST=true webpack --mode development",
    "prod-test": "cross-env IS_TEST=true webpack --mode production",
    "live-test": "cross-env IS_TEST=true webpack serve --open --mode development --hot",
    "mock-api": "nodemon express/mock-api-server.js",
    "dev:test": "concurrently \"npm run mock-api\" \"npm run dev-test\"",
    "prod:test": "concurrently \"npm run mock-api\" \"npm run prod-test\"",
    "live:test": "concurrently \"npm run mock-api\" \"npm run live-test\"",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write .",
    "eject": "react-scripts eject",
    "test": "jest",
    "test:with-mock-api": "cross-env USE_MOCK_API=true start-server-and-test mock-api http://127.0.0.1:5145/express-ready jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "autoprefixer-stylus": "^1.0.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-jsx-control-statements": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-webpack-plugin": "^5.0.2",
    "html-webpack-plugin": "^5.6.4",
    "install-webpack-plugin": "^4.0.6",
    "mini-css-extract-plugin": "^2.9.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "stylus": "^0.64.0",
    "stylus-loader": "^8.1.2",
    "terser-webpack-plugin": "^5.3.14",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^5.102.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-manifest-plugin": "^5.0.1",
    "webpack-pwa-manifest": "^4.3.0",
    "webpack-stream": "^7.0.0",
    "workbox-webpack-plugin": "^7.3.0"
  }
}
